{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static"
    },
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/src(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/src(.*)",
      "dest": "frontend/$1"
    }
  ]
}